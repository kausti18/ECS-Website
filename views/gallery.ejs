  <script>
    var darkButton=document.getElementById("dark");
    var checkbox=document.getElementById("checkbox");
    const currentTheme = localStorage.getItem("theme");
    console.log(currentTheme);
      function check(){
        var checkbox=document.getElementById("checkbox");
        const prefersDarkScheme = window.matchMedia("(prefers-color-scheme: dark)");
        // console.log("theme="+theme );
        if (currentTheme=="dark") {
            darkTheme();
            checkbox.setAttribute("checked","true");
          } else {
            console.log("Light Mode");
            checkbox.name="light";
          }
      }
  function darkTheme(){
      
    var bgImg=document.getElementsByClassName("bg-img");
    var bottom=document.getElementsByClassName("bottom");

    checkbox.name="dark";
    console.log(checkbox.name);
    var newTheme="dark";
    console.log("themeset="+newTheme);
    bgImg[0].style.backgroundImage="linear-gradient(#2d3436e6 0%, #000000e0 74%)";
            bottom[0].style.borderTopColor="white";
            bottom[0].style.backgroundImage="linear-gradient(0deg,#2d3436e6 0%, #000000e0 74%)";
    localStorage.setItem("theme",newTheme);
  }

//Change theme by checkbox
function test(){
var bgImg=document.getElementsByClassName("bg-img");
var bottom=document.getElementsByClassName("bottom");

if(checkbox.name=="light"){
            checkbox.name="dark";
            console.log(checkbox.name);
