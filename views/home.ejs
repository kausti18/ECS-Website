/*Gallery slideshow*/
*{
    box-sizing: border-box;
}

.home{
    overflow:hidden;
    position: relative;
    height: 25vw;width: 50vw;left: 25vw;border-color:#6f42c1;border-style:solid;
    border-radius: 2%;
    
}

.home .slider{
	left: 25vw;
}


.home .slide{
    position: absolute;
    width: 100%;
    height: 100%;
    background-size: cover;
    z-index:1; 
    display:none;
    animation: slide 2s ease;
    background-repeat: no-repeat;
}
.home .slide.active{
    display: flex;
}
@keyframes slide{
    0%{
        transform:scale(1.1);
    }
    100%{
        transform: scale(1);
    }
}

.home .controls .prev,
.home .controls .next{
 position: absolute;
 z-index:2;
 top:50%;
 height:40px;
 width: 40px;
 margin-top: -20px;
 color:#ffffff;
 background-color:#6f42c1;
 opacity: 0.7;
 text-align: center;
 line-height: 40px;
 font-size:20px;
 cursor:pointer;
 transition: all .5s ease;
}
.home .controls .prev:hover,
.home .controls .next:hover{
   opacity:1;
}
.home .controls .prev{
 left:0;
}
.home .controls .next{
 right:0;
}

.home .indicator{
    position: absolute;
    left:50%;
    bottom:5%;
    z-index: 2;
    transform: translateX(-50%);
}

.home .indicator div{
    display: inline-block;
    width:20px;
    height: 20px;
    color:transparent;
    background-color:transparent;
    border-color:white;
    border-style:solid;       
    border-radius:50%;
    text-align: center;
    line-height: 20px;
    margin:0 3px;
    transition: background-color 1s,border-color 1s;
    z-index: 2000;
}
.indicator div:hover{
  background-color:#6f42c1;
  border-style:solid; 
  border-color:white;   
} 


.home .indicator div.active{
 background-color:white;
}
